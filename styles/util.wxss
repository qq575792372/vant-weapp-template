/*
 * 页面全屏
 */
 .fullscreen {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  height: 100%;
  width: auto;
  display: block;
  overflow: hidden;
  z-index: 999;
  -webkit-overflow-scrolling: touch;
}

/**
 * 圆角样式
 */
.border-radius {
  border-radius: 4px !important;
}

.border-radius-round {
  border-radius: 100% !important;
}

.border-radius-ellipse {
  border-radius: 50px !important;
}

/*
* h1-h6
*/
.h1 {
  font-size: 36px;
}

.h2 {
  font-size: 30px;
}

.h3 {
  font-size: 24px;
}

.h4 {
  font-size: 18px;
}

.h5 {
  font-size: 14px;
}

.h6 {
  font-size: 12px;
}

.h1,
.h2,
.h3 {
  padding: 5px 0px;
}

.h4,
.h5,
.h6 {
  padding: 3px 0px;
}

/* 字体强调 */
.text-deleted {
  text-decoration: line-through;
}

.text-block {
  display: block;
}

.text-bold {
  font-weight: bold;
}

.text-bold-500 {
  font-weight: 500;
}

.text-smail {
  font-size: 85%;
}

.text-underline {
  border-bottom: solid 1px #4b4c50;
}

.text-uppercase {
  text-transform: uppercase;
}

.text-lowercase {
